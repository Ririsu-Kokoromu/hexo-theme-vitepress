<div class="tcPage">
    <div class="tcBox">
        <ul>
            <% site.tags.each(function(tag) { %>
                <li><a href="<%- url_for(tag.path) %>">
                        <%= tag.name %>
                    </a></li>
            <% }) %>
        </ul>
        <div class="postList">
            <% site.tags.forEach(function(tag) { %>
                <span><%= tag.name %></span>
                <% tag.posts.forEach(function(post) { %>
                    <p><a href="<%- url_for(post.path) %>"><%= post.title %></a></p>
                <% }) %>
            <% }) %>
        </div>
    </div>
</div>